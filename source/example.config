<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<appSettings>
		<!-- values: 1, 0, true, false -->
		<add key="Desharp:Enabled" value="1" />
		<!-- values: html, text -->
		<add key="Desharp:Output" value="html" />
		<!-- values: IPv4 or IPv6 separated by comma, only for web applications -->
		<add key="Desharp:DebugIps" value="127.0.0.1,88.31.45.67" />
		<!-- 
			values: list of keys bellow, 
			it's not necessary to use all, 
			all enabled by default
			if you want to disable any logging level - put minus char before level key
		-->
		<add key="Desharp:Levels" value="+exception,debug,info,-notice,-warning,+error,+critical,alert,+emergency,javascript" />
		<!-- 
			values: full class names extended from Desharp.Panels.Abstract, separated by comma
			build in panels: Desharp.Panels.Routing and Desharp.Panels.Session
		-->
		<add key="Desharp:Panels" value="Desharp.Panels.Routing,Desharp.Panels.Session" />
		<!-- values: absolute path or relative path from application root (relative path from app root has to start with '~/') -->
		<add key="Desharp:Directory" value="~/logs" />
		<!-- values: miliseconds how to often flush logged messages or exceptions from RAM to HDD -->
		<add key="Desharp:WriteMiliseconds" value="5000" />
		<!-- values: level depth do dump complex objects, 3 by default '~/' -->
		<add key="Desharp:Depth" value="3" />
		<!-- values: max length for dumped string values, 1024 by default -->
		<add key="Desharp:MaxLength" value="1024" />
		<!-- 
			values: any string key to open your editor from html output by: 
			`editor://file=...&line=...&editor=MSVS2015
			default values is by automaticly detected Visual Studio instalation in current system
		-->
		<add key="Desharp:Editor" value="MSVS2005" />
	</appSettings>
</configuration>